    "comment-parser": "^1.4.1",
    "crypto": "^1.0.1",
    "date-fns": "^3.3.1",
    "discord.js": "^14.8.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "form-data": "^4.0.0",
    "googleapis": "^120.0.0",
    "html-to-text": "^9.0.5",
    "jsdoc-api": "^8.0.0",
    "jsdoc-parse": "^6.2.0",
