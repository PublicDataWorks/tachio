{
  "name": "coachartie",
  "version": "1.0.1",
  "main": "bot.js",
  "license": "MIT",
  "scripts": {
    "start": "node bot.js",
    "web": "node web.js",
    "player": "nodemon capability-player.js",
    "manifest": "node capabilities/_generate-manifest.js",
    "test": "jest",
    "dev": "node --watch api.js",
    "linearlite:migrate": "npx pg-migrations apply --database postgresql://electric:${PROXY_PASSWORD}@localhost:65432/postgres --directory db/linearlite/migrations",
    "linearlite:migrate:docker": "npx pg-migrations apply --database postgresql://electric:${PROXY_PASSWORD}@electric:65432/postgres --directory db/linearlite/migrations",
    "tachio:migrate": "npx supabase --db-url postgresql://postgres:${POSTGRES_PASSWORD}@localhost:5432/postgres migration up",
    "tachio:migrate:docker": "npx supabase --workdir ./db --db-url postgresql://postgres:${POSTGRES_PASSWORD}@db:5432/postgres migration up"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.1",
    "@discordjs/voice": "^0.16.0",
    "@google-cloud/speech": "^5.5.0",
