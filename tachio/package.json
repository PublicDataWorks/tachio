    "@octokit/graphql": "^5.0.6",
    "@octokit/rest": "^19.0.11",
    "@supabase/supabase-js": "^2.42.0",
    "async-sema": "^3.1.1",
    "axios": "^1.4.0",
    "blessed": "^0.1.81",
    "boxen": "^7.1.1",
    "canvas": "^2.11.2",
    "chai": "^4.3.8",
    "chance": "^1.1.11",
    "cheerio": "^1.0.0-rc.12",
